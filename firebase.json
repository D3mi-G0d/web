{
  "functions": {
    "source": "functions"
  },
  "hosting": {
    "public": "public",
    "cleanUrls": true,
    // "redirects": [ {
    // "source": "/problem-statements",
    // "destination": "/coming",
    // "type": 301
    // }],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "emulators": {
    "functions": {
      "port": 5001
    },
    "firestore": {
      "port": 8080
    },
    "hosting": {
